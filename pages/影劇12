                        {
                            instances.push(instance);
                        }

                        instance.render();
                        if (node.ancestor(".yui3-ymsb .yui3-ymsb")) {
                            node.ancestor(".yui3-ymsb").setStyles({border:"0", padding:"0"});
                        }
                        instance = conf = id = null;
                    });
                }
            });

