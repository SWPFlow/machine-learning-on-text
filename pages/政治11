                                "fullpath":"https://s.yimg.com/nl/ynews/site/player.js"
                            }
                        }
                    }});
                    videoEmbedPlayer.init({
                        playerUrl : "https://s.yimg.com/nl/ynews/site/player.swf?lang=zh-tw",
                        autoPlay: "1"
                    });
                })(Y);
    });
    Y.later(10, this, function() {Y.namespace("Media").ywaSettings = '"projectId": "10001703162079", "documentName": "", "documentGroup": "storypage", "ywaColo" : "vscale3", "spaceId" : "2144404928" ,"customFields" : { "12" : "classic", "13" : "story" }';
                            Y.Media.YWA.init(Y.namespace("Media").ywaSettings);
    });
});
});
        
        </script>


